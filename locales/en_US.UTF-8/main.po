#: main.cpp:284
#, c-format
msgid ""
"Хелп:\n"
"\tinfo - о локации, go - идти,\n"
"\tПерегенерировать: loc - локацию по id, trouble - особенности, door - "
"двери, focus - фокус монстров\n"
"\tshow - сгенерить json, loot - потребовать с ДМа лут, magic - потребовать с "
"ДМа магический предмет\n"
"\tУправление: goal - целью, luck - удачей, map - качеством карты, level - "
"глубиной, pic - id карты для отображения\n"
"\tsave/load - сохранить/загрузить игру, quit - сдаться\n"
msgstr ""
"Help:\n"
"\tinfo - about location, go - go,\n"
"\tregenerate: loc - location by id, trouble - features, door - doors, focus "
"- monsters focus.\n"
"\tshow - generate json, loot - demand loot from DM, magic - demand magic "
"item from DM.\n"
"\tControls: goal - goal, luck - luck, map - map quality, level - depth, pic "
"- map id to display\n"
"\tsave/load - save/load game, quit - give up\n"

