#, c-format
msgid "Не удалось открыть файл на запись:(\n"
msgstr "Failed to open the file for writing:(\n"

#: save.cpp:57
#, c-format
msgid "Откуда: "
msgstr "From where: "

#: save.cpp:61
#, c-format
msgid "Не удалось открыть файл на чтение:(\n"
msgstr "Failed to open file for reading:(\n"
